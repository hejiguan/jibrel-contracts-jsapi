{
  "name": "jibrel-contracts-jsapi",
  "version": "0.1.0",
  "description": "High level toolbox for solving typical tasks when creating a web wallet.",
  "main": "index.js",
  "engines": {
    "node": "^6.10.0",
    "npm": "^3.10.10"
  },
  "scripts": {
    "compile": "npm run lint && npm run build && npm run minify",
    "build": "./node_modules/.bin/babel ./lib --out-file ./dist/jibrel-contracts-jsapi.js",
    "lint": "./node_modules/.bin/eslint ./lib",
    "minify": "./node_modules/.bin/minify ./dist/jibrel-contracts-jsapi.js --mangle -o ./dist/jibrel-contracts-jsapi.min.js",
    "testrpc": "./node_modules/.bin/testrpc",
    "test": "./node_modules/.bin/mocha --compilers babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jibrelnetwork/jibrel-contracts-jsapi.git"
  },
  "author": "Ivan Violentov <violentoff@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jibrelnetwork/jibrel-contracts-jsapi/issues"
  },
  "homepage": "https://github.com/jibrelnetwork/jibrel-contracts-jsapi#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-minify": "0.2.0",
    "babel-plugin-inline-json-import": "0.2.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "ethereumjs-testrpc": "4.1.0",
    "mocha": "3.5.0",
    "should": "11.2.1"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "crypto-js": "3.1.9-1",
    "elliptic": "6.4.0",
    "ethereumjs-tx": "1.3.3",
    "joi-browser": "10.6.1",
    "web3": "0.20.1"
  }
}
